{{search}}

<div class="container">
	<a href="/search" class="page-header-link">
		<h3 class="page-header">
			{{search.count}} results for '{{search.query}}'
		</h3>
		<a href="/movies/1" ng-href="/movies/{{result.ID}}" ng-repeat="result in search.results"><img class="col-xs-4 col-md-2 margin-vertical" ng-src="http://www.shostream.com/images/thumbs/films/original/{{result.ID}}_original.jpg"></img></a>
	</a>
</div>